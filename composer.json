{
    "name": "matt-daneshvar/laravel-survey",
    "description": "Create surveys inside your Laravel app",
    "keywords": [
        "laravel",
        "survey"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Daneshvar",
            "email": "matt.daneshvar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "laravel/legacy-factories": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "MattDaneshvar\\Survey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattDaneshvar\\Survey\\Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MattDaneshvar\\Survey\\SurveyServiceProvider"
            ]
        }
    }
}
